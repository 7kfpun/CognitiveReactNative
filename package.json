{
  "name": "Cognitive",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run override-modules && node node_modules/react-native/local-cli/cli.js start",
    "override-modules": "cp -rp override_node_modules/* node_modules",
    "build-android": "cd android && ./gradlew assembleRelease && cd .. && npm run say-done",
    "install-android": "npm run remove && cd android && ./gradlew installRelease && cd .. && npm run say-done",
    "remove": "adb uninstall com.cognitive && adb uninstall com.kfpun.cognitive",
    "say-done": "osascript -e 'display notification \"DONE!\" with title \"Android built\"'"
  },
  "dependencies": {
    "firebase": "^3.4.1",
    "react": "15.3.1",
    "react-motion": "^0.4.5",
    "react-native": "0.35.0",
    "react-native-admob": "^1.1.5",
    "react-native-animatable": "^0.6.1",
    "react-native-aws3": "0.0.2",
    "react-native-camera": "github:ibussieres/react-native-camera#issue-fix-ios10sim-crash",
    "react-native-device-info": "^0.9.5",
    "react-native-elements": "^0.6.1",
    "react-native-google-analytics-bridge": "^3.1.0",
    "react-native-image-resizer": "github:bamlab/react-native-image-resizer",
    "react-native-navbar": "^1.5.4",
    "react-native-permissions": "^0.2.5",
    "react-native-router-flux": "github:aksonov/react-native-router-flux",
    "react-native-share": "^1.0.16",
    "react-native-simple-store": "^1.1.0",
    "react-native-sleek-loading-indicator": "^0.1.2",
    "react-native-speech": "^0.1.2",
    "react-native-tableview-simple": "^0.13.0",
    "react-native-timer": "^1.1.2",
    "react-native-vector-icons": "^2.1.0"
  },
  "devDependencies": {
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.0"
  }
}
